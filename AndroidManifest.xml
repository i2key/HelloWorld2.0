<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.co.recruit.mtl.sample"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="10" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:icon="@drawable/ic_launcher"
        android:theme="@style/Theme.MyTheme"
        android:label="@string/app_name" >
        <activity
            android:name=".activity.QuerryActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.SimpleTimeLineActivity"></activity>
        <activity android:name=".activity.CustomTimeLineActivity"></activity>
        <activity android:name=".activity.TweetDetailActivity"></activity>
    </application>

</manifest>